{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": { "buildType": "apk" },
      "distribution": "internal",
      "env": {
        "BACKEND_IP": "https://clockaburra-restful-api.vercel.app"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "BACKEND_IP": "https://clockaburra-restful-api.vercel.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
